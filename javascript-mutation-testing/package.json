  "dependencies": {
    "conditional": "^5.3.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "expectations": "^0.5.2",
    "grunt": "^0.4.5",
    "grunt-mocha-test": "^0.12.7",
    "grunt-mutation-testing": "^1.3.0",
    "karma": "^0.13.11",
